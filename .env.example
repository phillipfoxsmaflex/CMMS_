# =============================================================================
# MMS (Maintenance Management System) Environment Configuration
# =============================================================================
# 
# IMPORTANT: This file must be copied to .env and customized for your deployment
# 
# URL CONFIGURATION GUIDE:
# ------------------------
# For deployment, you MUST update these URLs based on your setup:
#
# - PUBLIC_API_URL: The URL where the Nginx proxy is accessible (default: port 8080)
#   Examples:
#   - http://your-server-ip:8080
#   - http://your-domain.com:8080
#   - https://your-domain.com (if using SSL)
#
# - PUBLIC_FRONT_URL: The URL where the frontend is directly accessible (default: port 12000)
#   Examples:
#   - http://your-server-ip:12000
#   - http://your-domain.com:12000
#   Note: Most users will access via PUBLIC_API_URL (proxy on port 8080)
#
# =============================================================================

# Email Notifications
ENABLE_EMAIL_NOTIFICATIONS=false
INVITATION_VIA_EMAIL=false
SMTP_PWD=
SMTP_USER=
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587

# Database Configuration
POSTGRES_USER=rootUser
POSTGRES_PWD=mypassword

# Security
JWT_SECRET_KEY=your_jwt_secret_change_this_in_production
ALLOWED_ORGANIZATION_ADMINS=

# Google Services (Optional)
GOOGLE_KEY=
GOOGLE_TRACKING_ID=

# =============================================================================
# CRITICAL: Update these URLs for your deployment
# =============================================================================
# PUBLIC_API_URL should point to the Nginx proxy with /api path (port 8080)
# IMPORTANT: Must include /api at the end!
PUBLIC_API_URL=http://localhost:8080/api

# PUBLIC_FRONT_URL should point to the frontend direct access (port 12000)
PUBLIC_FRONT_URL=http://localhost:12000
GCP_BUCKET_NAME=
GCP_JSON=
GCP_PROJECT_ID=
MINIO_USER=minio
MINIO_PASSWORD=minio123
PUBLIC_MINIO_ENDPOINT=http://localhost:9000
MAIL_RECIPIENTS=
STORAGE_TYPE=MINIO
LICENSE_KEY=
ENABLE_SSO=false
OAUTH2_PROVIDER=
OAUTH2_CLIENT_ID=
OAUTH2_CLIENT_SECRET=
MUI_X_LICENSE=e0d9bb8070ce0054c9d9ecb6e82cb58fTz0wLEU9MzI0NzIxNDQwMDAwMDAsUz1wcmVtaXVtLExNPXBlcnBldHVhbCxLVj0y
LICENSE_FINGERPRINT_REQUIRED=false
